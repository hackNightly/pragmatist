	"tokenColors": [
		{
			"settings": {
				"foreground": "#ffffff",
				"background": "#ffffffff"
			}
		},
		{
			"name": "Italics",
			"scope": "emphasis, storage, comment, entity.other.attribute-name, entity.other.attribute-name.html, entity.other.attribute-name.tag.jade, entity.other.attribute-name.tag.pug, markup.italic, keyword.control, variable.language",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Bold",
			"scope": "strong, markup.bold, markup.heading",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#2E2C3D"
			}
		},
		{
			"name": "Foreground",
			"scope": "keyword.operator.class, constant.other, source.php.embedded.line",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Variable, String Link, Regular Expression, Tag Name",
			"scope": "variable, support.other.variable, string.other.link, string.regexp, entity.name.tag, meta.tag, declaration.tag",
			"settings": {
				"foreground": "#9AA2BA"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": "constant.numeric, constant.language, support.constant, constant.character, variable.parameter, punctuation.section.embedded, keyword.other.unit",
			"settings": {
				"foreground": "#A37D8C"
			}
		},
		{
			"name": "Class, Support",
			"scope": "entity.name.class, entity.name.type.class, support.type, support.class",
			"settings": {
				"fontStyle": "",
				"foreground": "#8778CC"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": "string, constant.other.symbol, entity.other.inherited-class, markup.heading",
			"settings": {
				"fontStyle": "",
				"foreground": "#75AA9A"
			}
		},
		{
			"name": "Punctuations",
			"scope": "punctuation.definition.block",
			"settings": {
				"foreground": "#6A7889"
			}
		},
		{
			"name": "Method declarations",
			"scope": "meta.method.declaration",
			"settings": {
				"foreground": "#A5B2BA"
			}
		},
		{
			"name": "Object Literal Key",
			"scope": "meta.object-literal.key.js",
			"settings": {
				"foreground": "#81B5B1"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": "keyword.operator, constant.other.color",
			"settings": {
				"foreground": "#3E999F"
			}
		},
		{
			"name": "Function, Special Method, Block Level",
			"scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.block-level",
			"settings": {
				"fontStyle": "",
				"foreground": "#9DB7AE"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": "keyword, storage, storage.type, entity.other.attribute-name",
			"settings": {
				"foreground": "#75AA9A"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#E0E0E0",
				"foreground": "#373B41"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}
